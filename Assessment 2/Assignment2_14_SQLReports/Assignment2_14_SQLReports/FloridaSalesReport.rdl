      <Query>
        <DataSourceName>SalesReportDataSource</DataSourceName>
        <CommandText>SELECT        Fact.Sale.[Total Including Tax], City_1.[State Province], Dimension.Date.Month, Dimension.Date.[Calendar Month Number], Dimension.Date.[Calendar Year]
FROM            Dimension.City AS City_1 INNER JOIN
                         Fact.Sale ON City_1.[City Key] = Fact.Sale.[City Key] INNER JOIN
                         Dimension.Date ON Fact.Sale.[Invoice Date Key] = Dimension.Date.Date</CommandText>
      </Query>
